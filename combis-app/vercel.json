 {
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "client/build" }
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2))",
        "dest": "/client/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/index.html"
      }
    ]
  }
